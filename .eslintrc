{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
  },
  "rules": {
    "no-console": 0,
    "disallowTabs": 0,
    "func-names": 0,
    "import/no-extraneous-dependencies": ["error", {
      devDependencies: true,
      optionalDependencies: false,
    }],
    "max-len": [2, {"code": 200, "tabWidth": 4, "ignoreUrls": true, "ignoreComments": true, "ignoreStrings": true}],
    "no-alert": 0,
    "no-underscore-dangle": 0,
    "no-shadow": 0,
    "no-tabs": 0,
    "no-undef": 0,
    "no-unused-vars": 0,
    "no-use-before-define": 0,
  }
}
